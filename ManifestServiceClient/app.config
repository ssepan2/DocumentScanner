<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="basic1manifest" messageEncoding="Mtom" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="ws1manifest" messageEncoding="Mtom" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:53408/PackageManifestService.svc/ws1"
                binding="wsHttpBinding" bindingConfiguration="ws1manifest"
