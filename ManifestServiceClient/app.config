            <endpoint address="http://localhost:53408/PackageManifestService.svc/basic1"
                binding="basicHttpBinding" bindingConfiguration="basic1manifest"
                contract="ManifestServiceClientReference.IPackageManifestService"
                name="basic1manifest" />
